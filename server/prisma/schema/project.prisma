model Project {
  id           Int          @id @default(autoincrement())
  owner        User         @relation("OwnedProjects", fields: [ownerId], references: [id])
  ownerId      Int
  participants User[]
  issues       Issue[]
  invitations  Invitation[]
  name         String       @unique
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
}
